{
  "name": "rendact-theme",
  "version": "0.1.0",
  "description": "Default theme for Rendact.",
  "author": "Rendact Team",
  "license": "GPL",
  "homepage": "https://github.com/rendact/rendact-theme-bootstrap#readme",
  "repository": "https://github.com/rendact/rendact-theme-bootstrap.git",
  "bugs": "https://github.com/rendact/rendact-theme-bootstrap/issues",
  "main": "./lib/Root.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0"
  },
  "dependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "scripts": {
    "build": "babel src -d lib --copy-files",
    "watch": "babel src -w -d lib --copy-files"
  }
}
